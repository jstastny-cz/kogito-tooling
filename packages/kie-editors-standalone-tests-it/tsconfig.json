{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ES6",
    "baseUrl": "./",
    "outDir": "dist",
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "experimentalDecorators": false,
    "downlevelIteration": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
